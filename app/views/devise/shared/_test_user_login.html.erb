<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <div class="test-user">
        <p><i class="fas fa-user"></i>テストユーザーとしてログインできます</p>
        <%= f.hidden_field :email, value: ENV['TEST_USER'] %>
        <%= f.hidden_field :password, value: ENV['TEST_USER_PASSWORD'] %>
        <%= f.submit 'テストログイン', class: 'btn btn-outline-dark' %>
      </div>
<% end %>