<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
<div class="test-user padding_down field">
  <%= f.hidden_field :email, value: ENV['TEST_USER'] %>
  <%= f.hidden_field :password, value: ENV['TEST_USER_PASSWORD'] %>
  <%= f.submit 'テストログイン(採用担当者様用)', class: 'btn btn-success' %>
</div>
<% end %>